{
  "name": "webpack_setup",
  "version": "0.0.1",
  "description": "webpack config setup, testing webpack configuration with webpack 2.x.x",
  "main": "server.js",
  "scripts": {
    "start": "SET NODE_ENV=production & npm run build:prod",
    "dev": "SET NODE_ENV=development && nodemon src/server/index.js --ignore src/common/",
    "clean": "rimraf dist/",
    "build:prod": "npm run clean && webpack -p"
  },
  "keywords": [
    "webpack",
    "webpack.config"
  ],
  "author": "Reuben Ringdal",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "express": "^4.15.2",
    "qs": "^6.4.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.1",
    "webpack": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "html-webpack-plugin": "^2.28.0",
    "npm-run-all": "^4.0.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  }
}
