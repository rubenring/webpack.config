{
  "name": "webpack_setup",
  "version": "0.0.1",
  "description": "webpack config setup, testing webpack configuration with webpack 2.x.x",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf server/",
    "dev": "npm run build & npm run watch",
    "watch": "nodemon server/main.bundle.js --watch server/",
    "build": "npm run clean && webpack --watch --progress",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-dev": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpack",
    "webpack.config"
  ],
  "author": "Reuben Ringdal",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "express": "^4.15.2",
    "webpack": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "npm-run-all": "^4.0.2",
    "webpack-node-externals": "^1.6.0"
  }
}
